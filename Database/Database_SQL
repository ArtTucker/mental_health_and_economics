-- Create database:
CREATE DATABASE Mental_Health_vs_Economics;

-- Suicide_rate table:
CREATE TABLE "Suicide_per_100K_individuals" (
    "Location" VARCHAR   NOT NULL,
    "Suicide_2019" FLOAT   NOT NULL,
    "Suicide_2018" FLOAT   NOT NULL,
    "Suicide_2017" FLOAT   NOT NULL,
    "Suicide_2016" FLOAT   NOT NULL,
    "Suicide_2015" FLOAT   NOT NULL,
    "Suicide_2014" FLOAT   NOT NULL,
    "Suicide_2013" FLOAT   NOT NULL,
    "Suicide_2012" FLOAT   NOT NULL,
    "Suicide_2011" FLOAT   NOT NULL,
    "Suicide_2010" FLOAT   NOT NULL
);

SELECT * FROM "Suicide_per_100K_individuals";

-- Machine Learning table:
CREATE TABLE "Machine_learning" (
    "Currently_employed_" INTEGER   NOT NULL,
    "Mental_Ilness" INTEGER   NOT NULL,
    "Education" VARCHAR   NOT NULL,
    "Computer_vs_smartphone" INTEGER   NOT NULL,
    "Hospitalized_mental_illness" INTEGER   NOT NULL,
    "Days_hospitalization" INTEGER   NOT NULL,
    "Legally_disabled" INTEGER   NOT NULL,
    "Access_to_internet" INTEGER   NOT NULL,
    "Live_with_parents" INTEGER   NOT NULL,
    "Gap_in_resume" INTEGER   NOT NULL,
    "Length_gaps_months" INTEGER   NOT NULL,
    "Annual_income" INTEGER   NOT NULL,
    "Unemployed" INTEGER   NOT NULL,
    "Read" INTEGER   NOT NULL,
    "Annual_income_from_welfare_programs" INTEGER   NOT NULL,
    "Food_stamps" INTEGER   NOT NULL,
    "Section_8_housing" INTEGER   NOT NULL,
    "Number_Hospitalization" INTEGER   NOT NULL,
    "Lack_concentration" INTEGER   NOT NULL,
    "Anxiety" INTEGER   NOT NULL,
    "Depression" INTEGER   NOT NULL,
    "Obsessive_thinking" INTEGER   NOT NULL,
    "Mood_swing" INTEGER   NOT NULL,
    "Panic_attack" INTEGER   NOT NULL,
    "Compulsive_behavior" INTEGER   NOT NULL,
    "Tiredness" INTEGER   NOT NULL,
    "Age" INTEGER   NOT NULL,
    "Gender" VARCHAR   NOT NULL,
    "Household_income" TEXT   NOT NULL,
    "Region" VARCHAR   NOT NULL,
    "Device_type" VARCHAR   NOT NULL
);

Drop TABLE "Machine_learning";

-- Create table Mental_health_workforce_avaibility:
CREATE TABLE "Mental_health_workforce_avaibility" (
    "Rank" INTEGER   NOT NULL,
    "State" VARCHAR   NOT NULL,
    "Rate" VARCHAR   NOT NULL
);

-- Check:
SELECT * FROM "Mental_health_workforce_avaibility";

-- Create income table:
CREATE TABLE "Income" (
    "lst" VARCHAR   NOT NULL,
    "geographic_area_name" VARCHAR   NOT NULL,
    "county" VARCHAR   NOT NULL,
    "state" VARCHAR   NOT NULL,
    "estimate_households_total" INTEGER   NOT NULL,
    "poor(<10K)" FLOAT   NOT NULL,
    "lower_poor(10K-15K)" FLOAT   NOT NULL,
    "middle_poor(15K-25K)" FLOAT   NOT NULL,
    "upper_poor(25K-35K)" FLOAT   NOT NULL,
    "working(35K-50K)" FLOAT   NOT NULL,
    "lower_middle(50K-75K)" FLOAT   NOT NULL,
    "middle(75K-100K)" FLOAT   NOT NULL,
    "upper_middle(100K-150K)" FLOAT   NOT NULL,
    "upper_elite(>200K)" FLOAT   NOT NULL,
    "median_income" INTEGER   NOT NULL
);

-- Create mental_vs_poverty table
CREATE TABLE "Mental_vs_poverty" (
    "state" VARCHAR   NOT NULL,
    "year" INTEGER   NOT NULL,
    "suicide_mean" FLOAT   NOT NULL,
    "poverty_percentage" FLOAT   NOT NULL,
    "mental_health_mean" FLOAT   NOT NULL,
    "SZ_score" FLOAT   NOT NULL,
    "PZ_score" FLOAT   NOT NULL,
    "MZ_score" FLOAT   NOT NULL
);

-- Check:
SELECT * FROM "Mental_vs_poverty";

-- Create Income_per_households table (from Danielle):
CREATE TABLE "Income_per_households" (
    "LST" VARCHAR   NOT NULL,
    "Geographic_Area_Name" VARCHAR   NOT NULL,
    "County" VARCHAR   NOT NULL,
    "id" INTEGER   NOT NULL,
    "Estimate_Households_Total" INTEGER   NOT NULL,
    "Estimate_Households_Total_Less_than_10,000" FLOAT   NOT NULL,
    "Estimate_Households_Total_$10,000_to_$14,999" FLOAT   NOT NULL,
    "Estimate_Households_Total_$15,000_to_$24,999" FLOAT   NOT NULL,
    "Estimate_Households_Total_$25,000_to_$34,999" FLOAT   NOT NULL,
    "Estimate_Households_Total_$35,000_to_$49,999" FLOAT   NOT NULL,
    "Estimate_Households_Total_$50,000_to_74,999" FLOAT   NOT NULL,
    "Estimate_Households_Total_$75,000_to_$99,999" FLOAT   NOT NULL,
    "Estimate_Households_Total_$100,000_to_$149,999" FLOAT   NOT NULL,
    "Estimate_Households_Total_$150,000_to_$199,999" FLOAT   NOT NULL,
    "Estimate_Households_Total_$200,000_or_more" FLOAT   NOT NULL,
    "EstimateHouseholds!!Median_income_(dollars)" INTEGER   NOT NULL,
    "Estimate_Families_Total" INTEGER   NOT NULL,
    "Estimate_Families_Total_Less_than_$10,000" FLOAT   NOT NULL,
    "Estimate_Families_Total_$10,000_to_$14,999" FLOAT   NOT NULL,
    "Estimate_Families_Total_$15,000_to_$24,999" FLOAT   NOT NULL,
    "Estimate_Families_Total_$25,000_to_$34,999" FLOAT   NOT NULL,
    "Estimate_Families_Total_$200,000_or_more" FLOAT   NOT NULL,
    "EstimateFamilies!!Median_income_(dollars)" INTEGER   NOT NULL,
    "Estimate_Families_Mean_income_$" INTEGER   NOT NULL,
    "EstimateFamiliesPERCENT_ALLOCATEDFamily_income_in_the_past_12_months" FLOAT   NOT NULL
);